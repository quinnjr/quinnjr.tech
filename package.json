  "scripts": {
    "ng": "ng",
    "start": "ng serve --watch --host 0.0.0.0 --live-reload --disable-host-check",
    "start:ssl": "ng serve --watch --host 0.0.0.0 --live-reload --disable-host-check --ssl --ssl-cert /certs/localhost+2.pem --ssl-key /certs/localhost+2-key.pem",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "cypress run",
    "test:cov": "ng test --coverage",
    "test:ci": "jest --runInBand",
    "dev:ssr": "ng run quinnjr-tech:serve-ssr",
    "serve:ssr": "node dist/quinnjr-tech/server/main.js",
    "build:ssr": "ng build && ng run quinnjr-tech:server",
    "prerender": "ng run quinnjr-tech:prerender",
    "prebuild:ssr": "ngcc",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prisma": "prisma",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "test:e2e-headless": "cypress run --headless",
    "postinstall": "husky install && prisma generate"
  },
